<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Administrar_Usuarios">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="121dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="123dp"
                android:layout_height="121dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="114dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="21dp"
                        android:orientation="vertical"></LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="24dp"
                            android:layout_height="match_parent"
                            android:orientation="horizontal"></LinearLayout>

                        <ImageView
                            android:id="@+id/imageView7"
                            android:layout_width="90dp"
                            android:layout_height="54dp"
                            android:layout_weight="1"
                            android:onClick="volver"
                            app:srcCompat="@drawable/volver" />
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="118dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="37dp"
                    android:orientation="vertical"></LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="85dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="17dp"
                        android:layout_height="61dp"
                        android:orientation="horizontal"></LinearLayout>

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="wrap_content"
                        android:layout_height="63dp"
                        android:layout_weight="1"
                        android:text="Administracion de usuarios" />
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="26dp"
                    android:orientation="vertical"></LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="39dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"></LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <RadioGroup
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" >

                                <RadioGroup
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent" >

                                    <RadioButton
                                        android:id="@+id/radioButton8"
                                        android:layout_width="match_parent"
                                        android:layout_height="35dp"
                                        android:text="Yordan Daniel tarazona" />

                                    <RadioButton
                                        android:id="@+id/radioButton9"
                                        android:layout_width="match_parent"
                                        android:layout_height="35dp"
                                        android:text="Yulieth Mayerly Vera" />

                                    <RadioButton
                                        android:id="@+id/radioButton10"
                                        android:layout_width="match_parent"
                                        android:layout_height="37dp"
                                        android:text="Joseph Harley Contreras" />

                                    <RadioButton
                                        android:id="@+id/radioButton11"
                                        android:layout_width="match_parent"
                                        android:layout_height="35dp"
                                        android:text="Alejandra Valencia Vega" />

                                    <RadioButton
                                        android:id="@+id/radioButton12"
                                        android:layout_width="match_parent"
                                        android:layout_height="37dp"
                                        android:text="Dylan Santiago Bautista" />

                                    <RadioButton
                                        android:id="@+id/radioButton13"
                                        android:layout_width="match_parent"
                                        android:layout_height="33dp"
                                        android:text="Maria Angelica Ortiz" />

                                    <RadioButton
                                        android:id="@+id/radioButton14"
                                        android:layout_width="match_parent"
                                        android:layout_height="32dp"
                                        android:text="Anderson Cardozo Arrieta" />

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:orientation="horizontal">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:orientation="horizontal">

                                            <LinearLayout
                                                android:layout_width="135dp"
                                                android:layout_height="match_parent"
                                                android:orientation="horizontal"></LinearLayout>

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="match_parent"
                                                android:orientation="vertical">

                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="20dp"
                                                    android:orientation="vertical"></LinearLayout>

                                                <Button
                                                    android:id="@+id/button2"
                                                    android:layout_width="159dp"
                                                    android:layout_height="wrap_content"
                                                    android:onClick="editar"
                                                    android:text="Editar " />
                                            </LinearLayout>
                                        </LinearLayout>
                                    </LinearLayout>
                                </RadioGroup>
                            </RadioGroup>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout >